<resources>
    <style name="home_button_interval">
        <item name="android:layout_marginRight">9dp</item>
        <item name="android:layout_marginLeft">9dp</item>
        <item  name="android:layout_marginBottom">9dp</item>
    </style>
   <style name="location_height">
       <item  name="android:layout_height">@dimen/DIMEN_90PX</item>
   </style>
   
   <style name="Dialoghome" parent="android:style/Theme.Dialog">
        <item name="android:background">#00000000</item>
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsFloating">true</item>
    </style>
   
  
   
   <style name="dialog" parent="@android:style/Theme.Dialog">
        <item name="android:windowFrame">@null</item><!--边框-->
        <item name="android:windowIsFloating">true</item><!--是否浮现在activity之上-->
        <item name="android:windowIsTranslucent">false</item><!--半透明-->
        <item name="android:windowNoTitle">true</item><!--无标题-->
       <!-- <item name="android:windowBackground">@color/alpha_bg</item>背景透明-->
        <item name="android:backgroundDimEnabled">false</item><!--模糊-->
    </style>
   
    <!--
        Base application theme, dependent on API level. This theme is replaced
        by AppBaseTheme from res/values-vXX/styles.xml on newer devices.
    -->
    <style name="AppBaseTheme" parent="android:Theme.Light">
        <!--
            Theme customizations available in newer API levels can go in
            res/values-vXX/styles.xml, while customizations related to
            backward-compatibility can go here.
        -->
    </style>

    <!-- Application theme. -->
    <style name="AppTheme" parent="AppBaseTheme">
        <!-- All customizations that are NOT specific to a particular API-level can go here. -->
    </style>
    
    
    <style name="LoadingDialog" parent="@android:style/Theme.Dialog">
        <item name="android:windowFrame">@null</item>
 <!-- 边框 -->
        <item name="android:windowIsFloating">true</item>
 <!-- 是否悬浮在activity上 -->
        <item name="android:windowBackground">@android:color/transparent</item>
 <!-- 背景透明 -->
        <item name="android:windowIsTranslucent">true</item>
 <!-- 半透明 -->
        <item name="android:windowNoTitle">true</item>
        <item name="android:backgroundDimEnabled">true</item>
 <!-- 模糊 -->
        <item name="android:backgroundDimAmount">0.6</item>
 <!-- 灰度 -->
    </style>

</resources>
